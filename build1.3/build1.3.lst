Nutrition-Cropping Optimization Model
Agents for calculation


 214  Set
 215     nn         'nutrition'         /calorie, protein, fat, carbonhydrate/;
 216   
 217  Parameter
 218     tpop                  'National population                (million)'
 219     pintake(a,g,nn)       'Daily intake requirement           (kcal grams/million)'
 220     nreq(nn)              'Daily intake requirement           (kcal grams/capita)'
 221     nnvalue(c,nn)         'Nutritive value of foods           (kcal grams/netfood 100g)'
 222     nnpc(c)               'Current daily intake of nutrients  (grams/capita)'
 223     t2g(c)                'Total food to gross food           (ratio)'
 224     t2p(c)                'Total food to processing use       (ratio)'
 225     t2f(c)                'Total food to feed use             (ratio)'
 226     g2n(c)                'Gross food to net food             (ratio)'
 227     x2n(c)                'Cropping area(1000ha) to net food(grams)               (ratio)'
 228     x2f(c,i)              'Cropping area(1000ha) to processed food(grams)         (ratio)'
 229     x2nn(c,nn)            'Cropping area(1000ha) to nutritive value(kcal grams)   (ratio)'
 230     x2fn(c,i,nn)          'Cropping area(1000ha) to nutritive value(kcal grams)   (ratio)'
 231  ;
 232   
 233  tpop = sum((a,g), intake(a,g,"pop"));
 234   
 235  pintake(a,g,"calorie") = intake(a,g,"pop") * intake(a,g,"calorie");
 236  pintake(a,g,"protein") = intake(a,g,"pop") * intake(a,g,"protein");
 237  pintake(a,g,"fat") = intake(a,g,"pop") * intake(a,g,"fat");
 238  pintake(a,g,"carbonhydrate") = intake(a,g,"pop") * intake(a,g,"carbonhydrate");
 239   
 240  nreq(nn) = sum((a,g),pintake(a,g,nn))/tpop;
 241   
 242  nnvalue(c,"calorie") = nvalue(c,"value","calorie");
 243  nnvalue(c,"protein") = nvalue(c,"value","protein");
 244  nnvalue(c,"fat") = nvalue(c,"value","fat");
 245  nnvalue(c,"carbonhydrate") = nvalue(c,"value","carbonhydrate");
 246   
 247  nnpc(c) = nvalue(c,"pc","supplypd");
 248   
 249  t2g(c) = data(c,"gross")/data(c,"total");
 250  t2p(c) = data(c,"processing")/data(c,"total");
 251  t2f(c) = data(c,"feed")/data(c,"total");
 252  g2n(c) = data(c,"g2n")/100;
 253  x2n(c)       = data(c,"yield")*t2g(c)*g2n(c)*(10**9);
 254  x2nn(c,nn)   = data(c,"yield")*t2g(c)*g2n(c)*(10**7)*nnvalue(c,nn);
 255  x2f(c,i)     = data(i,"yield")*t2p(i)*coefficient(c,i)*(10**9);
 256  x2fn(c,i,nn) = data(i,"yield")*t2p(i)*coefficient(c,i)*(10**7)*nnvalue(c,nn);
 257   
 258   
 259  Display tpop, pintake, nreq, nnvalue, nnpc, x2f, x2n;
