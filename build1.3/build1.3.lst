 328  Set
 329     crep  /
 330     landuse     'Area cropped               (1000ha)'
 331     supply      'Total food supply          (1000t)'
 332     grossfood   'Gross food supply          (1000t)'
 333     netfood     'Net food supply            (1000t)'
 334     current     'Current intake             (grams/capita)'
 335     potential   'Potential intake           (grams/capita)'
 336     change      'Change in diet             (grams/capita)'/
 337     nrep  /
 338     required    'Required daily nutrients   (kcal grams/capita)'
 339     intake      'Intaken nutrients per day  (kcal grams/capita)'
 340     shortage    'Shortage per day           (kcal grams/capita)'
 341     ssrate      'Self sufficient rate       (percentage)       '/;
 342   
 343  Parameter
 344     paddyrep     'Land report summary on paddy'
 345     fieldrep     'Land report summary on field'
 346     pasturerep   'Land report summary on pasture'
 347     orchardrep   'Land report summary on orchard'
 348     croprep      'Crop report summary'
 349     foodrep      'Processed food summary'
 350     dietrep      'Nutrient summary';
 351   
 352  paddyrep(t,c) = xcrop.l(c)*landreq(c,t) $ paddy(c);
 353  paddyrep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ paddy(c));
 354  fieldrep(t,c) = xcrop.l(c)*landreq(c,t) $ field(c);
 355  fieldrep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ field(c));
 356  pasturerep(t,c) = xcrop.l(c)*landreq(c,t) $ pasture(c);
 357  pasturerep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ pasture(c));
 358  orchardrep(t,c) = xcrop.l(c)*landreq(c,t) $ orchard(c);
 359  orchardrep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ orchard(c));
 360   
 361  croprep(c,"landuse")   = xcrop.l(c) $edible(c);
 362  croprep(c,"supply")    = xcrop.l(c)*data(c,"yield") $edible(c);
 363  croprep(c,"grossfood") = xcrop.l(c)*data(c,"yield")*t2g(c) $edible(c);
 364  croprep(c,"netfood")   = xcrop.l(c)*data(c,"yield")*t2g(c)*g2n(c) $edible(c);
 365  croprep(c,"current")   = nnpc(c)$edible(c);
 366  croprep(c,"potential") = croprep(c,"netfood")*(10**9)/(tpop*(10**6)*365);
 367  croprep(c,"change")    = croprep(c,"potential")-croprep(c,"current");
 368  croprep("total",crep)  = sum(c, croprep(c,crep));
 369   
 370  foodrep(c,"netfood")   = sum(i,xcrop.l(i)*data(i,"yield")*t2p(i)*coefficient(c,i));
 371  foodrep(c,"current")   = nnpc(c)$processed(c);
 372  foodrep(c,"potential") = foodrep(c,"netfood")*(10**9)/(tpop*(10**6)*365);
 373  foodrep(c,"change")    = foodrep(c,"potential")-foodrep(c,"current");
 374  foodrep("total",crep)  = sum(c, foodrep(c,crep));
 375   
 376  dietrep(nn,"required") = nreq(nn);
 377  dietrep(nn,"intake")   = sum(c,croprep(c,"netfood")*nnvalue(c,nn)*(10**7))   /(tpop*(10**6)*365)
 378                         + sum(c,foodrep(c,"netfood")*nnvalue(c,nn)*(10**7))   /(tpop*(10**6)*365)
 379                         + sum(c,data(c,"net")*nnvalue(c,nn)*(10**7)$marine(c))/(tpop*(10**6)*365);
 380  dietrep(nn,"shortage") = dietrep(nn,"intake") - dietrep(nn,"required");
 381  dietrep(nn,"ssrate")   = 100*dietrep(nn,"intake") / dietrep(nn,"required");
