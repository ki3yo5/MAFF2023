Nutrition-Cropping Optimization Model
Agents for calculation


 218  Set
 219     nn         'nutrition'         /calorie, protein, fat, carbonhydrate/;
 220   
 221  Parameter
 222     tpop                  'National population                (million)'
 223     pintake(a,g,nn)       'Daily intake requirement           (kcal grams/million)'
 224     nreq(nn)              'Daily intake requirement           (kcal grams/capita)'
 225     nnvalue(c,nn)         'Nutritive value of foods           (kcal grams/netfood 100g)'
 226     nnpc(c)               'Current daily intake of nutrients  (grams/capita)'
 227     t2g(c)                'Total food to gross food           (ratio)'
 228     t2p(c)                'Total food to processing use       (ratio)'
 229     t2f(c)                'Total food to feed use             (ratio)'
 230     g2n(c)                'Gross food to net food             (ratio)'
 231     x2n(c)                'Cropping area(1000ha) to net food(grams)               (ratio)'
 232     x2f(c,i)              'Cropping area(1000ha) to processed food(grams)         (ratio)'
 233     x2nn(c,nn)            'Cropping area(1000ha) to nutritive value(kcal grams)   (ratio)'
 234     x2fn(c,i,nn)          'Cropping area(1000ha) to nutritive value(kcal grams)   (ratio)'
 235  ;
 236   
 237  tpop = sum((a,g), intake(a,g,"pop"));
 238   
 239  pintake(a,g,"calorie") = intake(a,g,"pop") * intake(a,g,"calorie");
 240  pintake(a,g,"protein") = intake(a,g,"pop") * intake(a,g,"protein");
 241  pintake(a,g,"fat") = intake(a,g,"pop") * intake(a,g,"fat");
 242  pintake(a,g,"carbonhydrate") = intake(a,g,"pop") * intake(a,g,"carbonhydrate");
 243   
 244  nreq(nn) = sum((a,g),pintake(a,g,nn))/tpop;
 245   
 246  nnvalue(c,"calorie") = nvalue(c,"value","calorie");
 247  nnvalue(c,"protein") = nvalue(c,"value","protein");
 248  nnvalue(c,"fat") = nvalue(c,"value","fat");
 249  nnvalue(c,"carbonhydrate") = nvalue(c,"value","carbonhydrate");
 250   
 251  nnpc(c) = nvalue(c,"pc","supplypd");
 252   
 253  t2g(c) = data(c,"gross")/data(c,"total");
 254  t2p(c) = data(c,"processing")/data(c,"total");
 255  t2p("soy") = 18/data("soy","prod");
 256  t2p("psoy")= 18/data("psoy","prod");
 257  t2f(c) = data(c,"feed")/data(c,"total");
 258  g2n(c) = data(c,"g2n")/100;
 259  x2n(c)       = data(c,"yield")*t2g(c)*g2n(c)*(10**9);
 260  x2nn(c,nn)   = data(c,"yield")*t2g(c)*g2n(c)*(10**7)*nnvalue(c,nn);
 261  x2f(c,i)     = data(i,"yield")*t2p(i)*coefficient(c,i)*(10**9);
 262  x2fn(c,i,nn) = data(i,"yield")*t2p(i)*coefficient(c,i)*(10**7)*nnvalue(c,nn);
 263   
 264  Display tpop, pintake, nreq, nnvalue, nnpc, x2f, x2n;
