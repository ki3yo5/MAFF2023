 339  Set
 340     crep  /
 341     landuse     'Area cropped               (1000ha)'
 342     supply      'Total food supply          (1000t)'
 343     grossfood   'Gross food supply          (1000t)'
 344     netfood     'Net food supply            (1000t)'
 345     current     'Current intake             (grams/capita)'
 346     potential   'Potential intake           (grams/capita)'
 347     change      'Change in diet             (grams/capita)'/
 348     nrep  /
 349     required    'Required daily nutrients   (kcal grams/capita)'
 350     intake      'Intaken nutrients per day  (kcal grams/capita)'
 351     shortage    'Shortage per day           (kcal grams/capita)'
 352     ssrate      'Self sufficient rate       (percentage)       '/;
 353   
 354  Parameter
 355     paddyrep     'Land report summary on paddy'
 356     fieldrep     'Land report summary on field'
 357     pasturerep   'Land report summary on pasture'
 358     orchardrep   'Land report summary on orchard'
 359     croprep      'Crop report summary'
 360     foodrep      'Processed food summary'
 361     dietrep      'Nutrient summary';
 362   
 363  paddyrep(t,c) = xcrop.l(c)*landreq(c,t) $ paddy(c);
 364  paddyrep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ paddy(c));
 365  fieldrep(t,c) = xcrop.l(c)*landreq(c,t) $ field(c);
 366  fieldrep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ field(c));
 367  pasturerep(t,c) = xcrop.l(c)*landreq(c,t) $ pasture(c);
 368  pasturerep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ pasture(c));
 369  orchardrep(t,c) = xcrop.l(c)*landreq(c,t) $ orchard(c);
 370  orchardrep(t,"total") = sum(c,xcrop.l(c)*landreq(c,t) $ orchard(c));
 371   
 372  croprep(c,"landuse")   = xcrop.l(c) $edible(c);
 373  croprep(c,"supply")    = xcrop.l(c)*data(c,"yield") $edible(c);
 374  croprep(c,"grossfood") = xcrop.l(c)*data(c,"yield")*t2g(c) $edible(c);
 375  croprep(c,"netfood")   = xcrop.l(c)*data(c,"yield")*t2g(c)*g2n(c) $edible(c);
 376  croprep(c,"current")   = nnpc(c)$edible(c);
 377  croprep(c,"potential") = croprep(c,"netfood")*(10**9)/(tpop*(10**6)*365);
 378  croprep(c,"change")    = croprep(c,"potential")-croprep(c,"current");
 379  croprep("total",crep)  = sum(c, croprep(c,crep));
 380   
 381  foodrep(c,"netfood")   = sum(i,xcrop.l(i)*data(i,"yield")*t2p(i)*coefficient(c,i));
 382  foodrep(c,"current")   = nnpc(c)$processed(c);
 383  foodrep(c,"potential") = foodrep(c,"netfood")*(10**9)/(tpop*(10**6)*365);
 384  foodrep(c,"change")    = foodrep(c,"potential")-foodrep(c,"current");
 385  foodrep("total",crep)  = sum(c, foodrep(c,crep));
 386   
 387  dietrep(nn,"required") = nreq(nn);
 388  dietrep(nn,"intake")   = sum(c,croprep(c,"netfood")*nnvalue(c,nn)*(10**7))   /(tpop*(10**6)*365)
 389                         + sum(c,foodrep(c,"netfood")*nnvalue(c,nn)*(10**7))   /(tpop*(10**6)*365)
 390                         + sum(c,data(c,"net")*nnvalue(c,nn)*(10**7)$marine(c))/(tpop*(10**6)*365);
 391  dietrep(nn,"shortage") = dietrep(nn,"intake") - dietrep(nn,"required");
 392  dietrep(nn,"ssrate")   = 100*dietrep(nn,"intake") / dietrep(nn,"required");
 393   
