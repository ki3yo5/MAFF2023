Nutrition-Cropping Optimization Model
Endogenous Variables and Equations


 269  Variable
 270     xcrop(c)      'Cropping area                     (1000ha)'
 271     ndeficit(nn)  'Deficit of nutrients   (kcal grams/capita)'
 272     cdeficit      'Deficit of calorie     (kcal      /capita)'
 273     delta(c)      'Change in diet         (     grams/capita)'
 274     target        'Target value to minimize' ;
 275   
 276  Positive Variable xcrop;
 277   
 278  Equation
 279     ybal(t)       'Land balance on paddy             (1000ha)'
 280     fbal(t)       'Land balance on upland            (1000ha)'
 281     pbal(t)       'Land balance on pasture           (1000ha)'
 282     obal(t)       'Land balance on orchard           (1000ha)'
 283     aybal(c)      'Cropping area balance on paddy    (1000ha)'
 284     afbal(c)      'Cropping area balance on upland   (1000ha)'
 285     apbal(c)      'Cropping area balance on pasture  (1000ha)'
 286     aobal(c)      'Cropping area balance on orchard  (1000ha)'
 287     albal(c)      'Cropping area balance on local    (1000ha)'
 288     nbal(nn)       'Nutrients balance      (kcal grams/capita)'
 289     calorie       'Calorie balance        (kcal      /capita)'
 290     diet(c)       'Change in diet         (     grams/capita)'
 291     obj           'Objective function'
 292     alt           'Alternative objective function';
 293   
 294  ybal(t)..    sum(c, xcrop(c)*landreq(c,t) $ paddy(c)) =l= lpaddy;
 295   
 296  fbal(t)..    sum(c, xcrop(c)*landreq(c,t) $ field(c)) =l= lfield;
 297   
 298  pbal(t)..    sum(c, xcrop(c)*landreq(c,t) $ pasture(c)) =l= lpasture;
 299   
 300  obal(t)..    sum(c, xcrop(c)*landreq(c,t) $ orchard(c)) =l= lorchard;
 301   
 302  aybal(c)..   xcrop(c) $ paddy(c) =l= aweight*data(c,"area") $ paddy(c);
 303   
 304  afbal(c)..   xcrop(c) $ field(c) =l= aweight*data(c,"area") $ field(c);
 305   
 306  apbal(c)..   xcrop(c) $ pasture(c) =l= (10**6)*data(c,"area") $ pasture(c);
 307   
 308  aobal(c)..   xcrop(c) $ orchard(c) =l= data(c,"area") $ orchard(c);
 309   
 310  albal(c)..   xcrop(c) $ local(c) =l= (aweight/2)*data(c,"area") $ local(c);
 311   
 312  nbal(nn)..   ndeficit(nn) =e= nreq(nn) - sum(c,xcrop(c)*x2nn(c,nn))/(tpop*(10**6)*365)
 313                                         - sum((i,c),xcrop(i)*x2fn(c,i,nn))/(tpop*(10**6)*365)
 314                                         - sum(c,data(c,"net")*nnvalue(c,nn)*(10**7)$marine(c))/(tpop*(10**6)*365);
 315   
 316  calorie..    cdeficit =e= ndeficit("calorie");
 317   
 318  diet(c)..    delta(c) =e= nnpc(c) - xcrop(c)*x2n(c)/(tpop*(10**6)*365)
 319                                    - sum(i,xcrop(i)*x2f(c,i))/(tpop*(10**6)*365)
 320                                    - data(c,"net")*(10**9)$marine(c)/(tpop*(10**6)*365);
 321   
 322  * denominator for (1-cweight): count items that take non-zero values or 7 food groups
 323   
 324  obj..    target =e= cweight*cdeficit + sum(c,((1-cweight)/7)*delta(c));
 325   
 326  alt..    target =e= cweight*cdeficit/nreq("calorie") + sum(c,((1-cweight)/7)*delta(c)/(nnpc(c)+0.001));
 327   
 328  Model presim_dif 'difference' / ybal,fbal,pbal,obal,aybal,afbal,apbal,aobal,albal,nbal,calorie,diet,obj /;
 329  Model presim_rat 'rate'       / ybal,fbal,pbal,obal,aybal,afbal,apbal,aobal,albal,nbal,calorie,diet,alt /;
 330   
 331  if(1,
 332  solve presim_rat minimizing target using nlp ;
 333  else
 334  solve presim_dif minimizing target using nlp ;)
 335   
